{
  "name": "offerrims",
  "version": "2.1.1",
  "description": "offer先生招聘信息管理系统",
  "private": true,
  "scripts": {
    "start": "cross-env APP_TYPE=site umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "dev": "cross-env APP_TYPE=site umi dev",
    "dev:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "node ./scripts/prettier.js",
    "commit": "git-cz"
  },
  "dependencies": {
    "ant-design-pro": "^2.1.1",
    "antd": "^3.10.9",
    "classnames": "^2.2.6",
    "dva": "^2.4.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^4.0.0",
    "moment": "^2.22.2",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "query-string": "^6.0.0",
    "rc-animate": "^2.4.4",
    "react": "^16.6.3",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-media": "^1.8.0",
    "umi-request": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.7",
    "commitlint-config-cz": "^0.11.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "inquirer": "^6.2.2",
    "jest-puppeteer": "^4.0.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "puppeteer": "^1.12.2",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "umi": "^2.3.0-0",
    "umi-plugin-pro-block": "^1.2.0",
    "umi-plugin-react": "^1.3.0-0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./commit.config.js"
    }
  }
}
